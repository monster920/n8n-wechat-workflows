        ],
        "id": "45003b81-ff87-4d01-94b1-7d7756b38e57",
        "name": "标题生成"
      },
      {
        "parameters": {
          "jsCode": "// 适配实际输入格式：提取【文章主题】【内容核心】+ 数字.开头的标题 + 统一处理所有字段转义符\nconst items = $input.all();\n\n// 辅助函数1：解码字符串中的转义字符（如\\\\n→\\n，用于提取逻辑）\nfunction unescapeString(str) {\n  if (!str) return \"\";\n  return str.replace(/\\\\n/g, \"\\n\").replace(/\\\\r/g, \"\\r\").replace(/\\\\t/g, \"\\t\");\n}\n\n// 辅助函数2：编码字符串中的换行/制表符（如\\n→\\\\n，用于输出格式）\nfunction escapeString(str) {\n  if (!str) return \"\";\n  return str.replace(/\\n/g, \"\\\\n\").replace(/\\r/g, \"\\\\r\").replace(/\\t/g, \"\\\\t\");\n}\n\nfor (const item of items) {\n  // 1. 读取原始output并解码（把\\\\n转成实际的\\n，方便按行提取）\n  const originalOutput = item.json[\"output\"] || \"\";\n  const decodedOutput = unescapeString(originalOutput);\n\n  // 2. 按实际换行符拆分，过滤空行，用于内容提取\n  const contentLines = decodedOutput.split(\"\\n\").map(line => line.trim()).filter(line => line);\n\n  // 3. 初始化提取字段\n  let articleTheme = \"\";\n  let contentCore = \"\";\n  const titles = [];\n\n  // 4. 逐行精准提取【文章主题】【内容核心】【标题】\n  contentLines.forEach(line => {\n    // 提取【文章主题】\n    if (line.includes(\"【文章主题】\")) {\n      articleTheme = line.split(\"【文章主题】：\")[1]?.trim() || \"\";\n    }\n    // 提取【内容核心】\n    else if (line.includes(\"【内容核心】\")) {\n      contentCore = line.split(\"【内容核心】：\")[1]?.trim() || \"\";\n    }\n    // 提取数字.开头的标题\n    else if (/^\\d+\\./.test(line)) {\n      const title = line.replace(/^(\\d+)\\./, \"$1.  \").trim(); // 保留“数字.  标题”格式\n      titles.push(title);\n    }\n  });\n\n  // 5. 统一处理所有输出字段的转义符（\\n→\\\\n）\n  // 处理原始output字段：先解码再重新编码，确保转义符格式统一\n  item.json[\"output\"] = escapeString(decodedOutput);\n  // 处理提取的自定义字段\n  item.json[\"文章主题\"] = escapeString(articleTheme);\n  item.json[\"内容核心\"] = escapeString(contentCore);\n  item.json[\"标题\"] = escapeString(titles.join(\"\\n\")) || \"\";\n}\n\nreturn items;"
        },
