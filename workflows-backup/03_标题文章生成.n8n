    {
      "parameters": {
        "jsCode": "/**\n * 替换字符串中的转义字符为显式转义形式\n * @param {string} input - 需要处理的原始字符串\n * @returns {string} 替换后的字符串\n */\nfunction escapeSpecialChars(input) {\n    if (!input || typeof input !== 'string') {\n        return input || '';\n    }\n    \n    const escapeMap = {\n        '\\n': '\\\\n',\n        '\\r': '\\\\r',\n        '\\t': '\\\\t',\n        '\\b': '\\\\b',\n        '\\f': '\\\\f',\n        '\\\\': '\\\\\\\\'\n    };\n    \n    return input.replace(/[\\n\\r\\t\\b\\f\\\\]/g, match => escapeMap[match]);\n}\n\n// n8n中处理输入并提取标题+内容（去掉内容开头的\\\\n）\nconst items = $input.all();\nfor (const item of items) {\n    // 1. 读取原始输入（对应“output”字段）\n    const originalString = item.json[\"output\"] || '';\n    // 2. 处理转义字符\n    const processedString = escapeSpecialChars(originalString);\n    \n    // 3. 提取标题（#开头的第一行）和内容（剩余部分，去掉开头的\\\\n）\n    const lines = processedString.split('\\\\n'); // 按转义后的换行符分割\n    let title = '';\n    let content = '';\n    \n    if (lines.length > 0) {\n        // 标题：取#开头的第一行（保留#格式）\n        title = lines[0].trim(); \n        // 内容：取第一行之后的部分 → 先拼接，再去掉开头的所有\\\\n → 最后去空\n        content = lines.slice(1)\n                       .join('\\\\n')       // 拼接剩余行\n                       .replace(/^\\\\n+/, '') // 核心优化：去掉开头的一个/多个\\\\n\n                       .trim();           // 去掉首尾多余空格\n    }\n\n    // 4. 写入输出字段\n    item.json[\"标题\"] = title;\n    item.json[\"内容\"] = content;\n}\n\nreturn items;"
      },
